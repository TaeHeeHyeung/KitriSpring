package com.kitri.hello2;

public class HelloServiceKor implements HellowService {
	
	public String helloKor(String name) {
		return name+"님 안녕하세요.";
	}
	public static void main(String[] args) {
		HelloServiceKor helloServiceKor = new HelloServiceKor();
		new HelloServiceKor().helloKor("태희");
	}

}
